{
    "name": "iqom/iqom-translation-provider",
    "description": "iQom translation provider for Symfony",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Iqom\\TranslationProviderBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Leonid Zakharov",
            "email": "leo@iqom.ru"
        }
    ],
    "minimum-stability": "stable",
    "license": "proprietary",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "iqom/iqom-client": "^1.1",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/yaml": "7.1.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/iqom/"
        }
    }
}
